{
  "name": "modular-admin-react",
  "version": "0.0.1",
  "description": "ModularAdmin: Free Dashboard Theme; React version",
  "repository": {
    "type": "git",
    "url": "https://github.com/modularcode/modular-admin-react.git"
  },
  "author": "ModularCode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/modularcode/modular-admin-react/issues"
  },
  "scripts": {
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "start": "npm run build && npm run server:prod",
    "build": "npm run client:prod",
    "prebuild": "del-cli public/client -f",
    "client:dev": "webpack-dev-server --config tools/webpack.config.dev.js  --hot --history-api-fallback --open",
    "client:prod": "webpack --config tools/webpack.config.prod.js",
    "client:lint": "eslint --config .eslintrc.client src/client/",
    "server:dev": "nodemon src/server/main.js --watch \"server/**/*.js\" ",
    "server:prod": "node src/server/main.js",
    "server:lint": "eslint --config .eslintrc.server src/server/"
  },
  "engines": {
    "node": ">=7"
  },
  "homepage": "https://github.com/modularcode/modular-admin-react",
  "dependencies": {
    "express": "^4.15.0",
    "grid-styled": "^1.0.1",
    "normalizr": "^3.2.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-hot-loader": "^1.3.1",
    "react-intl": "^2.2.3",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-form": "^6.5.0",
    "redux-logger": "^2.8.2",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4",
    "styled-components": "^1.4.4"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "concurrently": "^3.4.0",
    "del-cli": "^0.2.1",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "html-webpack-plugin": "^2.28.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0"
  }
}
