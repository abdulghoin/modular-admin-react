{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "func-names": ["error", "never"],
    "indent": [
      "warn",
      2
    ],
    "quotes": [
      "error",
      "single"
    ],
    "react/prefer-stateless-function": [0]
  },
  "plugins": [
    "react"
  ]
}
